<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://www.mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="com.gSpring.dao.MemberDao">
	<select id="getMemberAll" resultType="MemberDto" parameterType="java.lang.String">
		SELECT * 
		FROM USERS
		where USER_NAME = #{name}
	</select>
</mapper>